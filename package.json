  "scripts": {
    "yarn": "yarn",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "podInstall": "cd ios && pod install && cd ..",
    "autoInstall": "yarn && jetify && npm run podInstall && npm run getSvg",
    "npmAutoInstall": "npm install && jetify && npm run podInstall && npm run getSvg",
    "getSvg": "node ./src/svgs/getSvg.js",
    "ipa-release": "xcodebuild -workspace ios/QHWallet.xcworkspace -scheme QHWallet archive",
    "apk-beta-release": "cd android && ./gradlew assembleReleaseStaging",
    "apk-release": "cd android && ./gradlew assembleRelease",
    "apk-release-cmd": "cd android && gradlew.bat assembleRelease",
    "bundle-ios-output": "react-native bundle --entry-file ./index.js --bundle-output ./bundle/ios/main.jsbundle --platform ios --assets-dest ./bundle/ios --dev false",
    "bundle-android-output": "react-native bundle --entry-file ./index.js --bundle-output ./bundle/android/main.jsbundle --platform android --assets-dest ./bundle/android --dev false"
  },
